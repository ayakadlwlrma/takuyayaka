<div class="tweets-container">
    <% @genres.each do |t| %>
        <div class="tweet">
            <%= t.song %>
            <%= t.singer %>
        </div>
        <%= link_to "詳細へ", genre_path(t.id) %>
        <%= link_to "編集する", edit_genre_path(t.id) %>
        <%= button_to "削除する", genre_path(t.id), method: :delete %> 
    <% end %>
</div>

<%= link_to "新規投稿へ", new_genre_path %>
